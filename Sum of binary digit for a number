import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int n=scan.nextInt();
		int i=0,count=0;
		int binary[]=new int[100];
		while(n>0)
		{
		    binary[i]=n%2;
		    n/=2;
		    i++;
		}
		for(int j=0;j<binary.length;j++)
		{
		    if(binary[j]==1)
		    {
		        count++;
		    }
		}
		System.out.println(count);
	}
}
